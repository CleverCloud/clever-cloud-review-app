{
    "compilerOptions": {
      "target": "es6",
      "module": "NodeNext",
      "moduleResolution": "nodenext",
      "outDir": "dist",
      "rootDir": "src",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
       "forceConsistentCasingInFileNames": true
     },
     "include": ["src"]
   }
  